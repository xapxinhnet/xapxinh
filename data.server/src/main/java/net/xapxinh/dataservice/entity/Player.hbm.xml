<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    
	<class name="net.xapxinh.dataservice.entity.Player" table="players">
		
		<id name="id" type="long" column="id">
			<generator class="native" />
		</id>
				
		<property name="mac">
			<column name="mac" />
		</property>
			
		<property name="licenseKey">
			<column name="license_key" />
		</property>
		
		<property name="acceptRequest">
			<column name="accept_request" />
		</property>
		
		<property name="connectedTime">
			<column name="connected_time" />
		</property>
		
	</class>
	
</hibernate-mapping>